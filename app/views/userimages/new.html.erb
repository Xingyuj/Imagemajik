<h1>Create a new image</h1>
<%= form_for @userimage do |f| %>
    <p>Title: <%= f.text_field :title,class:"form-control" , style:"width:50%"%></p>
    <p>Description: <%= f.text_area :description ,class:"form-control", style:"width:50%"%></p>
    <p><%= f.file_field :image, class: "btn btn-default" %></p>
    <%= f.submit "Submit", class: "btn btn-primary" %>
    <%= link_to 'Back', my_userimages_path, class: "btn btn-default" %>
<% end %>

